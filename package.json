{
  "name": "angularjs-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate for angular.js 1.6 using components, firebase, and webpack.",
  "main": "index.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint src/**.js",
    "test": "npm run lint",
    "build": "node_modules/.bin/cross-env NODE_ENV=production NODE_ENV=production node_modules/.bin/webpack --config build/webpack.conf.js",
    "build:dev": "node_modules/.bin/webpack --config build/webpack.conf.js",
    "dev": "./node_modules/.bin/webpack-dev-server --config build/webpack.dev.conf.js --hot --port 3000 --host 0.0.0.0",
    "start": "./node_modules/.bin/webpack-dev-server --config build/webpack.base.conf.js --cache --host 0.0.0.0 --port 3000 --public todo.aboutjn.xyz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neetVeritas/angularjs-webpack-boilerplate.git"
  },
  "keywords": [
    "angularjs",
    "javascript",
    "app",
    "webpack",
    "sass",
    "scss",
    "boilerplate",
    "firebase"
  ],
  "author": "John Nolette",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neetVeritas/angularjs-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/neetVeritas/angularjs-webpack-boilerplate#readme",
  "devDependencies": {
    "angularjs-template-loader": "^0.1.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.1.1",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.0.0",
    "npm": "^5.0.4",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "angular": "^1.6.5",
    "angular-ui-router": "^1.0.3",
    "angularfire": "^2.3.0",
    "animate.css": "^3.5.2",
    "font-awesome": "^4.7.0",
    "skeleton-css": "^2.0.4"
  }
}
